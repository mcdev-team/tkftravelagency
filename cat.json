            "id": "utils-waitForDependencies",
            "type": "inline",
            "run": true,
            "once": true,
            "code": "(function(){window.waitForDependencies=function(deps,callback,attempts=20,delay=250){if(!Array.isArray(deps)||deps.length===0)return callback();const allReady=deps.every(dep=>{if(typeof dep==='function')return dep();else if(typeof dep==='string'){const parts=dep.split('.');let obj=window;for(let part of parts){if(typeof obj[part]==='undefined')return false;obj=obj[part];}return true;}return false;});if(allReady){callback();}else if(attempts>0){setTimeout(()=>window.waitForDependencies(deps,callback,attempts-1,delay),delay);}else{console.warn('Dependencies not satisfied:',deps);}}})();"
